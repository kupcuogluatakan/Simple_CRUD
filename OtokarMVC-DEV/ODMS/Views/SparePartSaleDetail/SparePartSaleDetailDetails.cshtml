@using ODMSCommon.Resources

@model ODMSModel.SparePartSaleDetail.SparePartSaleDetailDetailModel
@{
    ViewBag.Title = MessageResource.SparePartSaleDetail_PageTitle_Details;
    Layout = "~/Views/Shared/Popup_Layout.cshtml";
}
<link href="~/Style/bootstrap.css" rel="stylesheet" />
<script>
    $('body').css('overflow', 'hidden');
</script>
<div class="labelDiv">@Html.LabelFor(d => d.SparePartName)</div>
<div class="shortTxtDiv">@Html.DisplayFor(d => d.SparePartName)&nbsp;</div>

<div class="labelDiv">@Html.LabelFor(d => d.SparePartCode)</div>
<div class="shortTxtDiv">@Html.DisplayFor(d => d.SparePartCode)&nbsp;</div>

<div class="clearDiv">&nbsp;</div>
<div class="labelDiv">@Html.LabelFor(d => d.ListPrice)</div>
<div class="shortTxtDiv">@Html.DisplayFor(d => d.ListPrice) @Html.DisplayFor(e => e.DealerCurrencyCode)&nbsp;</div>

<div class="labelDiv">@Html.LabelFor(d => d.DealerPrice)</div>
<div class="shortTxtDiv">@Html.DisplayFor(d => d.DealerPrice) @Html.DisplayFor(e => e.DealerCurrencyCode)&nbsp;</div>

<div class="clearDiv">&nbsp;</div>
