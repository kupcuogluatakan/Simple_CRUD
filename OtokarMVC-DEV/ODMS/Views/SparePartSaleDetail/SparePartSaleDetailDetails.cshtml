<div class="labelDiv">@Html.LabelFor(d => d.PickedQuantity)</div>
<div class="shortTxtDiv">@Html.DisplayFor(d => d.PickedQuantity)&nbsp;</div>
<div class="clearDiv">&nbsp;</div>
