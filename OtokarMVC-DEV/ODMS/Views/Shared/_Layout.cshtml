                .center().open();

            kendoWindow
                .find(".delete-confirm,.delete-cancel")
                .click(function () {
                    if ($(this).hasClass("delete-confirm")) {
                        confirm = true;
                        if (customFunction)
                            customFunction();
                    }
                    else {
                        resetChooseLoader();
                    }
                    kendoWindow.data("kendoWindow").close();
                })
                .end();
            return confirm;
        }

       


