        $(document).ready(tagInput);

        function tagInput() {
            $('.tagInput').tagsinput({
                tagClass: 'tagInputsCss'
            });

            setTimeout(setTagInput(), 150);
        }

        function setTagInput(){
            $('.tagInput').prev().find("input").attr("tagInput", "1");
        }

    </script>

</body>
</html>
