            Globalize.culture('tr-TR');
            var $search = $('#searchDiv');
            var resetButtonHtml;
            resetButtonHtml = "<button class='k-button' style='font-weight:bold !important;' id='btnReset' onclick='ResetSearch();'><i class=' glyphicon glyphicon-remove'></i> " + '@MessageResource.Global_Display_Clear' + "</button>";
            $search.after(resetButtonHtml);
            //searchHtml = $search.html();
            $search.keypress(function (e) {
                var code = e.keyCode || e.which;
                if (code == 13) {

                    var isTagInput = 0;
                    $.each(e.target.attributes, function () {
                        if (this.nodeName == "taginput") {
                            isTagInput = 1;
                        }
                    });

                    if (isTagInput == 0) {
                        $('#btnSearch').trigger("click");
                    }
                }
            });
            $("body").delegate("div.k-overlay", "click", overlayClick);
        });

        function ResetSearch() {
            //$('#searchDiv').html(searchHtml);
            $('#searchDiv :input').each(function () {
                if (!$(this).attr('disabled')) {//Orhan: Disabled alanların clear edilmemesi için.(Örn: Bayi)
                    if ($(this).is(':checkbox'))
                        $(this).removeAttr('checked');
                    else
                        $(this).val('');
                }

            });
            $(".k-datepicker", '#searchDiv').width(200).css("padding", "0");
        }



        function overlayClick() {
            var wnd = $("div.k-overlay + div.k-window:visible div").eq(2);
            var kwnd = wnd.data("kendoWindow");
            if (kwnd) {
                kwnd.close();
                wnd.html('');
            }
        }

        function DeleteConfirm(deleteFunction) {
            var confirm = false;
            var kendoWindow = $("<div />").kendoWindow({
                title: "@MessageResource.Gloabl_Display_DeleteConfirmTitle",
                resizable: false,
                modal: true
            });

            kendoWindow.data("kendoWindow")
                .content($("#delete-confirmation").html())
                .center().open();

            kendoWindow
                .find(".delete-confirm,.delete-cancel")
                .click(function () {
                    if ($(this).hasClass("delete-confirm")) {
                        confirm = true;
                        if (deleteFunction)
                            deleteFunction();
                    }
                    kendoWindow.data("kendoWindow").close();
                })
                .end();
            return confirm;
        }
