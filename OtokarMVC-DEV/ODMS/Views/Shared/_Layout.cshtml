                        traditional: true,
                        dataType: "json",
                        success: function (result) {
                            window.location = '@Url.Action("Login", "SystemAdministration")';
                            //if (result.Status == 0)
                            //    SetErrorMessage(result.Message);
                            //else
                            //    SetSuccessMessage(result.Message);
                        }

                    });
