@{
    ViewBag.Title = MessageResource.GuaranteeAuthorityGroup_PageTitle_Update;
    Layout = "~/Views/Shared/Popup_Layout.cshtml";
}
<style>
    #popupContentWrapper {
        min-height: 250px !important;
    }
</style>
@using ODMSCommon.Resources
@using ODMSModel.GuaranteeAuthorityGroup
@model GuaranteeAuthorityGroupViewModel
@using (Html.BeginForm("GuaranteeAuthorityGroupUpdate", "GuaranteeAuthorityGroup", FormMethod.Post))
{
    Html.RenderPartial("_GuaranteeAuthorityGroup", Model);
    <input type="hidden" name="GroupId" id="GroupId" value="@Model.GroupId"/>
    @ODMSHelpers.Button("Update", CommonUtility.GetResourceValue("Global_Display_Update"), CommonValues.PermissionCodes.GuaranteeAuthorityGroup.GuaranteeAuthorityGroupUpdate, "GuaranteeAuthorityGroupUpdate")
}