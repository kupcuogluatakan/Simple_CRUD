@helper GetLabourTdClass(sbyte id, bool isExternal)
{
if (isExternal)
{
        @Html.Raw(" success ")
    return;
}

if (id == -1 || id == 0)
{
        @Html.Raw(" danger ")
    return;
}
else if (id == 4)
{
        @Html.Raw(" success ")
    return;
}
else
{
        @Html.Raw(" warning ")
}
}

